<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.imsglobal.basiclti.consumersecret.model.OauthConsumerSecret" table="basiclti_consumer_secret">
		<id name="consumerKey" column="consumer_key" type="java.lang.String" length="64">
			<generator class="assigned"/>
		</id>
		<property name="consumerSecret" column="consumer_secret" length="64"/>		
	</class>
</hibernate-mapping>